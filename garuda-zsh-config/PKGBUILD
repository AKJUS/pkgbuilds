pkgname=garuda-zsh-config
pkgver=1.1.11
pkgrel=1
pkgdesc="Zsh configuration for Garuda Linux"
arch=(any)
url="https://gitlab.com/garuda-linux/themes-and-settings/settings/$pkgname"
license=('GPL')
conflicts=('grml-zsh-config' 'manjaro-zsh-config')
depends=('fzf'
  'garuda-starship-prompt'
  'mcfly'
  'ugrep'
  'zsh'
  'zsh-autosuggestions'
  'zsh-completions'
  'zsh-history-substring-search'
  'zsh-syntax-highlighting')
makedepends=('git')
source=('zshrc')
sha256sums=('7f7ba0ce00c97febe1d2beb9193da806acc2652554b12e1522beb55533ac11a2')

package() {
  install -D -m644 zshrc "$pkgdir/etc/skel/.zshrc"
}
